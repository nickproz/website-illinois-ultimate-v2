{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "Angular2/Webpack dependencies",
  "scripts": {
    "start": "npm run build && webpack-dev-server --inline --hot --progress",
    "build:prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "rimraf dist && webpack",
    "git-publish": "npm run build:prod; gh-pages -d dist",
    "pretty": "prettier --print-width 150 --tab-width 4 --arrow-parens always --single-quote --write \"src/**\""
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "angular2-google-maps": "^0.17.0",
    "core-js": "^2.4.1",
    "hover.css": "^2.2.0",
    "jquery": "3.1.1",
    "lightbox2": "^2.9.0",
    "rxjs": "5.0.0-beta.12",
    "semantic-ui": "2.2.10",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/es6-shim": "0.31.32",
    "@types/jquery": "2.0.45",
    "@types/node": "0.0.1",
    "@types/semantic-ui": "2.2.3",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "css-to-string-loader": "^0.1.2",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.14.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "typescript": "2.1.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1"
  }
}
